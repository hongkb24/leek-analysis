{
    "name": "leek-analysis",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "eslint": "eslint --fix --ext .js,.ts,.vue ./src",
        "prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
        "lint-staged": "lint-staged",
        "commit": "git add -A && czg && git push"
